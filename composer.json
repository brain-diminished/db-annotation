{
    "name": "brain-diminished/db-annotation",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dorian Savina",
            "email": "dorian.savina@gmail.com"
        }
    ],
    "require": {
        "doctrine/dbal": "^2.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "satooshi/php-coveralls": "^1.0.1"
    },
    "autoload": {
        "psr-4": {
            "BrainDiminished\\DBAnnotation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BrainDiminished\\Test\\DBAnnotation\\": "tests/"
        }
    },
    "archive": {
        "exclude": ["tests", "phpunit.xml.dist"]
    }
}
